{
    "name": "flixerr",
    "version": "1.5.0",
    "description": "A movie torrent streaming service.",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "build": "babel ./assets/js --out-dir dist/js",
        "package-win": "electron-packager . Flixerr --platform=win32 --arch=x64 --asar --prune --icon=./assets/imgs/icon.ico --out=./build/ --overwrite",
        "package-darwin": "electron-packager . Flixerr --darwin-x64 --asar --prune --icon=./assets/imgs/icon.icns --out=./build/ --overwrite",
        "package-linux": "electron-packager . Flixerr --platform=linux --arch=x64 --asar --prune --icon=./assets/imgs/icon.png --out=./build/ --overwrite",
        "create-installer-windows": "node build.js",
        "create-installer-mac": "electron-installer-dmg ./build/Flixerr-darwin-x64/Flixerr.app Flixerr --out=./build --background=./assets/imgs/installer-bg.png --icon=./assets/imgs/external-drive.icns --overwrite",
        "create-installer-linux": "electron-installer-debian --src ./build/Flixerr-linux-x64/ --dest ./build/installers/ --arch amd64",
        "do-everything": "npm run build && npm run package-win && npm run package-darwin && npm run package-linux && npm run create-installer-mac && npm run create-installer-linux && npm run create-installer-windows"
    },
    "author": "Flixerr",
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "electron": "^5.0.2",
        "electron-installer-dmg": "^2.0.0",
        "electron-packager": "^12.2.0",
        "electron-installer-debian": "^2.0.0",
        "electron-winstaller": "^3.0.4"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "babel-register": "^6.26.0",
        "cheerio": "^1.0.0-rc.3",
        "decompress": "^4.2.0",
        "download": "^7.1.0",
        "electron-json-storage": "^4.1.6",
        "electron-squirrel-startup": "^1.0.0",
        "electron-titlebar": "0.0.3",
        "electron-window-state": "^5.0.3",
        "fast-average-color": "^5.1.0",
        "firebase": "^6.0.2",
        "franc-min": "^4.1.0",
        "fs-extra": "^8.1.0",
        "get-json": "^1.0.1",
        "https-proxy-agent": "^2.2.1",
        "langs": "^2.0.0",
        "parse-torrent-name": "^0.5.4",
        "pluralize": "^8.0.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-lazy-load": "^3.0.13",
        "react-reveal": "^1.2.2",
        "react-transition-group": "^1.2.1",
        "torrent-search-api": "^2.0.8",
        "uniqid": "^4.1.1",
        "webtorrent": "^0.103.1",
        "webtorrent-health": "^1.1.2"
    }
}
